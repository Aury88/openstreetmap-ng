{
  "name": "openstreetmap",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.3.2",
    "iD": "github:openstreetmap/iD#release",
    "js-cookie": "^3.0.0",
    "leaflet": "^1.8.0",
    "leaflet.locatecontrol": "^0.79.0",
    "osm-community-index": "^5.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "onchange": "^7.1.0",
    "postcss-cli": "^11.0.0"
  },
  "scripts": {
    "build:sass": "sass --style compressed --load-path node_modules --no-source-map static/sass:static/css",
    "prefix:css": "postcss static/css/**/*.css --use autoprefixer --replace --no-map",
    "sass": "onchange 'static/sass/**/*.scss' -- npm run build:sass && npm run prefix:css"
  },
  "browserslist": [
    "defaults"
  ]
}
