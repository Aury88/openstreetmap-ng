    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: load_osm.py",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "scripts/load_osm.py",
            "args": [
                "poland-internal.osm",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
